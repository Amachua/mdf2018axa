{
    "presets": [
        "es2015",
        ["env", {
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
                "node": "6"
            }
        }]
    ],
    "plugins": [
        "lodash",
        "transform-object-assign",
        "transform-object-rest-spread",
        "syntax-object-rest-spread",
        "transform-es2015-parameters",
        "transform-class-properties",
        "array-includes"
    ]
}